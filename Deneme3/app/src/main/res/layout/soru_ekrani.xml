<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:ignore="UselessParent">

        <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/editText6"
            android:layout_below="@+id/editText5"
            android:layout_alignLeft="@+id/editText5"
            android:layout_alignStart="@+id/editText5" />

        <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:layout_marginTop="11dp"
            android:id="@+id/editText5"
            android:layout_alignParentTop="true"
            android:layout_toRightOf="@+id/textView5"
            android:layout_toEndOf="@+id/textView5"
            android:elevation="0dp"
            android:cursorVisible="false"
            tools:targetApi="lollipop" />

        <Button
            android:text="Kaydet"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="17dp"
            android:id="@+id/button4"
            android:layout_alignParentBottom="true"
            android:layout_alignLeft="@+id/textView6"
            android:layout_alignStart="@+id/textView6"
            tools:ignore="HardcodedText" />

        <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="date"
            android:ems="10"
            android:id="@+id/editText7"
            android:layout_below="@+id/editText6"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />

        <TextView
            android:text="dogumtarihi"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView5"
            android:textSize="20sp"
            tools:ignore="HardcodedText"
            android:layout_below="@+id/textView4"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <TextView
            android:text="Soyad"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView4"
            android:textSize="24sp"
            tools:ignore="HardcodedText"
            android:layout_below="@+id/textView2"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <TextView
            android:text="ad"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView2"
            android:textSize="30sp"
            tools:ignore="HardcodedText,UnknownId"
            android:layout_alignBottom="@+id/textView10"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <TextView
            android:text="Hobileriniz:"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="72dp"
            android:id="@+id/textView6"
            android:textSize="24sp"
            tools:ignore="HardcodedText"
            android:layout_below="@+id/editText7"
            android:layout_toRightOf="@+id/textView4"
            android:layout_toEndOf="@+id/textView4" />

        <CheckBox
            android:text="Voleybol"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/checkBox1"
            android:layout_alignRight="@+id/editText6"
            android:layout_alignEnd="@+id/editText6"
            android:layout_marginRight="20dp"
            android:layout_marginEnd="20dp"
            android:id="@+id/checkBox2"
            tools:ignore="HardcodedText" />

        <CheckBox
            android:text="Basketbol"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/checkBox2"
            android:layout_toRightOf="@+id/checkBox2"
            android:layout_toEndOf="@+id/checkBox2"
            android:layout_marginLeft="26dp"
            android:layout_marginStart="26dp"
            android:id="@+id/checkBox3"
            tools:ignore="HardcodedText" />

        <CheckBox
            android:text="Kitap Okumak"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/checkBox5"
            android:layout_alignLeft="@+id/checkBox3"
            android:layout_alignStart="@+id/checkBox3"
            android:id="@+id/checkBox6"
            tools:ignore="HardcodedText" />

        <CheckBox
            android:text="Gezmek"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/checkBox5"
            android:layout_alignBaseline="@+id/checkBox4"
            android:layout_alignBottom="@+id/checkBox4"
            android:layout_alignLeft="@+id/checkBox2"
            android:layout_alignStart="@+id/checkBox2"
            tools:ignore="HardcodedText" />

        <CheckBox
            android:text="Online Oyun"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="19dp"
            android:id="@+id/checkBox4"
            android:layout_below="@+id/checkBox1"
            android:layout_alignLeft="@+id/checkBox1"
            android:layout_alignStart="@+id/checkBox1"
            tools:ignore="HardcodedText" />

        <CheckBox
            android:text="Futbol"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="67dp"
            android:id="@+id/checkBox1"
            android:layout_below="@+id/textView6"
            android:layout_alignLeft="@+id/textView6"
            android:layout_alignStart="@+id/textView6"
            tools:ignore="HardcodedText" />

        <CheckBox
            android:text="Kamp Yapmak"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_alignLeft="@+id/checkBox4"
            android:layout_alignStart="@+id/checkBox4"
            android:id="@+id/checkBox7"
            tools:ignore="HardcodedText" />

    </RelativeLayout>

</LinearLayout>
